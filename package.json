{
  "name": "my-store-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "preview": "npm run build && vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint --ignore-path .gitignore --fix src",
    "prettier": "prettier src --write",
    "bootstrap": "cd infrastructure && cdk bootstrap",
    "deploy": "npm run build && npm run deploy:cdk && npm run invalidate-cache",
    "deploy:cdk": "cd infrastructure && cdk deploy",
    "invalidate-cache": "node scripts/invalidate-cache.js",
    "destroy": "cd infrastructure && cdk destroy"
  },
  "dependencies": {
    "@aws-cdk/aws-cloudfront": "^1.203.0",
    "@aws-cdk/aws-cloudfront-origins": "^1.203.0",
    "@aws-cdk/aws-s3": "^1.203.0",
    "@aws-cdk/aws-s3-deployment": "^1.203.0",
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.16.14",
    "@mui/styles": "^5.16.14",
    "@types/yup": "^0.29.14",
    "aws-cdk-lib": "^2.178.2",
    "axios": "^1.7.9",
    "constructs": "^10.4.2",
    "formik": "^2.2.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-query": "^3.39.1",
    "react-router-dom": "^6.3.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@aws-sdk/client-cloudfront": "^3.750.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.2.1",
    "@types/aws-lambda": "^8.10.147",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "@vitejs/plugin-react": "^1.3.2",
    "@vitest/ui": "^0.18.0",
    "c8": "^7.11.3",
    "child_process": "^1.0.2",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "fs": "^0.0.1-security",
    "jsdom": "^20.0.0",
    "msw": "^2.7.0",
    "path": "^0.12.7",
    "prettier": "2.7.1",
    "ts-node": "^10.9.2",
    "typescript": "^4.7.4",
    "vite": "^6.1.0",
    "vitest": "^0.17.1"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
